{
  "version": 2,
  "env": {
    "DATABASE_URL": "@database-url" 
  },
  "functions": {
    "api/random-number.js": {
      "runtime": "vercel/node@16"
    }
  },
  "routes": [
    {
      "src": "/api/random-number",
      "dest": "/api/random-number.js"
    }
  ],
  "crons": [
    {
      "path": "/api/cron",
      "schedule": "0 0 * * *" 
    }
  ]
}

